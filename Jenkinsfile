pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Buidling Code'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Code'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Code'
            }
        }
        stage('Release') {
            steps {
                echo 'Code Release'
            }
        }
    }
}
